<Window x:Class="Tema2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tema2"
        mc:Ignorable="d" ResizeMode="NoResize"
        Title="MainWindow" Height="550" Width="900">
    <Grid RenderTransformOrigin="0.566,0.559">
        <CheckBox x:Name="checkBox" Margin="523,277,346,222"/>
        <TextBlock Text="Multiple jumps" Width="80" Height="30" Margin="355,232,365,172" ></TextBlock>
        <Button x:Name="LoadGame" Content="Load game" Margin="405,338,405,156" Click="LoadGame_Click"></Button>
        <Button x:Name="ShowWins" Content="See the statistics"  Margin="385,430,385,65" Click="ShowWins_Click"></Button>
        <Button x:Name="StartNewGame" Content="New Game" Margin="405,186,405,308" Click="StartNewGame_Click"></Button>
        
    </Grid>
</Window>
